{
  "env": {
    "browser": true,
    "es2021": true,
    "node" : true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "import/extensions": ["error", "always"],
    "import/no-unresolved": "off",
    "@typescript-eslint/no-unused-vars": [2, { "args": "none" }]
  }
}
