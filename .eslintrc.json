{
  "env": {
    "browser": true,
    "es6": true,
    "node" : true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "import/extensions": ["error", "always"],
    "import/no-unresolved": "off"
  }
}
